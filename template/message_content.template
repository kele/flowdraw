<div msg_id="{{msg_id}}" class="popup" style="display: none;">
    <div class="handle"></div><br>
    <strong style="padding: 5px;">{{msg_id}}. {{msg_label}} [{{sender}} ({{full_sender}}) -> {{receiver}} ({{full_receiver}})]</strong>
    <br>
    <div class="popup_callstack_button"><strong>Callstack</strong></div>
    <div class="popup_callstack">
<pre>{{msg_callstack}}</pre>
    <br>
    </div>
    <div class="popup_content">
<pre>{{msg_content}}</pre>
    </div>
</div>
